{
  "name": "@shelf/froala-image-upload-s3-signature",
  "version": "1.0.1",
  "description": "Get S3 signature for Froala editor with configurable expiration time",
  "license": "MIT",
  "repository": "shelfio/froala-image-upload-s3-signature",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Vitalii Sikora",
    "email": "vitalii@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "rm -rf lib/ && yarn build:types && yarn build:code",
    "build:code": "babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts' && find ./lib -name '*.test.d.ts' -delete",
    "build:types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --declarationDir lib",
    "coverage": "yarn test --coverage",
    "lint": "yarn lint:ci --fix",
    "lint:ci": "eslint . --ext .js,.ts,.json",
    "prepack": "yarn build",
    "test": "TZ=UTC jest src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "main": "lib",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "keywords": [
    "wysiwyg-editor",
    "s3-signature",
    "froala"
  ],
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@shelf/eslint-config": "2.16.0",
    "@types/node": "8",
    "eslint": "8.12.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ],
    "*.{html,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}
